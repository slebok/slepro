grammar : {rule}* ;
rule : {'[' label ']'}? nonterminal ':' symbols ';' ;
[t] symbol : terminal ;
[n] symbol : nonterminal ;
[star] symbol : '{' symbols '}' '*' ;
[plus] symbol : '{' symbols '}' '+' ;
[option] symbol : '{' symbols '}' '?' ;
symbols : {symbol}* ;
label : name ;
terminal : qstring ;
nonterminal : name ;
