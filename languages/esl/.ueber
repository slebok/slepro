:- load('applied-term.pro').
:- load('as.pro').
:- load('conformance.pro').
:- load('api.pro').
:- load('cs-to-as.pro').

:- interpret(elementOf, [term], appliedTerm, []).
:- interpret(elementOf, [esl(text)], eglElementOf(eslMapping), ['cs.term', 'ls.term']).
:- interpret(elementOf, [esl(term)], appliedSignature, []).

:- elementOf('as.term', esl(term)).
:- elementOf('cs.egl', egl(text)).
:- elementOf('cs.term', egl(term)).
:- elementOf('ls.egl', egl(text)).
:- elementOf('ls.term', egl(term)).

:- mapsTo(parseEgl, 'cs.egl', 'cs.term').
:- mapsTo(parseEgl, 'ls.egl', 'ls.term').
