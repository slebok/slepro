type ueber = udecl* ;
symbol language : lang -> udecl ;
symbol membership : lang x goal x file* -> udecl ;
symbol equivalence : lang x goal x file* -> udecl ;
symbol function : func x lang+ x lang+ x goal x file* -> udecl ;
