[
  % Syntax and sample
  macro(eglSyntax(yafpl)),
  macro(eslSyntax(yafpl)),
  macro(parse(yafpl)),

  % Wellness
  language(yafpl(ok(term))),
  membership(yafpl(ok(term)), okProg, []),
  elementOf('sample.term', yafpl(ok(term))),

  % Results of interpretation
  language(yafpl(value(term))),
  language(yafpl(normal(term))),
  membership(yafpl(value(term)), value, []),
  membership(yafpl(normal(term)), normal, []),
  elementOf('sample.value', yafpl(value(term))),
  elementOf('sample.normal', yafpl(normal(term))),

  % Interpretation
  mapsTo(bigStep, ['sample.term'], ['sample.value']),
  mapsTo(smallStep, ['sample.term'], ['sample.normal']),
  mapsTo(denotational, ['sample.term'], ['sample.value'])

].
